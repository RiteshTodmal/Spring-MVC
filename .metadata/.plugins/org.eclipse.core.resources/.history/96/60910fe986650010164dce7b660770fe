package com.ninfinity.aspect;

import org.aspectj.lang.annotation.Aspect;
import org.aspectj.lang.annotation.Before;

@Aspect
public class MyAspect {

	@Before("execute(* com.ninfinity.services.PaymentService.makePayment())")
	public void printBefore() {
		System.out.println("Authentication Process Initiated");
	}
}
